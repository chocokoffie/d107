<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>wiki-entries-file</param-name>
        <param-value>resources/entries.csv</param-value>
    </context-param>

    <context-param>
        <param-name>all-calendar-items-file</param-name>
        <param-value>resources/all-items.json</param-value>
    </context-param>

    <context-param>
        <param-name>calendar-items-by-room-file</param-name>
        <param-value>resources/items-by-room.json</param-value>
    </context-param>

</web-app>
